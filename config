# https://docs.gitlab.com/ee/user/ssh.html

# GitHub Identity
Host github.com
	HostName github.com
	PreferredAuthentications publickey
	IdentityFile ~/.ssh/SSH_Key

# GitLab Identity
Host gitlab.com
	Hostname gitlab.com
	PreferredAuthentications publickey
	IdentityFile ~/.ssh/SSH_Key

# Azure Identity
Host ssh.dev.azure.com
	IdentityFile ~/.ssh/SSH_Key
	IdentitiesOnly yes

# Local Network
Host SAMSUNG1 samsung1 XPS1 xps1 pi1 PI1
	IdentityFile ~/.ssh/SSH_Key
	IdentitiesOnly yes