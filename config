# https://docs.gitlab.com/ee/user/ssh.html

# GitLab Identity
Host gitlab.com
  Hostname gitlab.com
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/SSH_Key

# GitHub Identity
Host github.com
  IdentityFile ~/.ssh/SSH_Key
  IdentitiesOnly yes

# Azure Identity
Host ssh.dev.azure.com
  IdentityFile ~/.ssh/SSH_Key
  IdentitiesOnly yes